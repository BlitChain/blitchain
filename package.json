{
  "name": "@blitchain/blitjs",
  "version": "0.0.1",
  "description": "Blit javascript client",
  "author": "Sybil Singleton <sybil.singleton@protonmail.com>",
  "homepage": "https://github.com/blitchain/blitjs#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "blit-client-ts"
  },
  "files": [
    "dist",
    "!CHANGELOG.md"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json --outDir dist --module commonjs || true",
    "build:mjs": "tsc -p tsconfig.json --outDir mjs --module es2022 --declaration false || true",
    "clean:mjs": "rimraf mjs",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:mjs && npm run clean:dist",
    "build:rename": "publish-scripts --cmd rename --srcDir mjs --outDir dist --findExt js --replaceExt mjs --no-rmDir",
    "build": "npm run clean && npm run build:cjs && npm run build:mjs && npm run build:rename",
    "codegen": "node scripts/codegen.js",
    "prepare": "npm run build",
    "lint": "eslint blit-client-ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blitchain/blitchain"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/blitchain/blitchain/issues"
  },
  "devDependencies": {
    "@cosmology/telescope": "^1.0.1",
    "@protobufs/osmosis": "^0.4.0",
    "@types/jest": "^29.5.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-in-case": "^1.0.2",
    "prettier": "^2.8.7",
    "publish-scripts": "0.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@cosmjs/amino": "0.29.4",
    "@cosmjs/proto-signing": "0.29.4",
    "@cosmjs/stargate": "0.29.4",
    "@cosmjs/tendermint-rpc": "^0.29.4"
  }
}
